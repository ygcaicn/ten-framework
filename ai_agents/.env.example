# Extension: cosy_tts
# Cosy TTS key
COSY_TTS_KEY=

# Speechmatics API key
SPEECHMATICS_API_KEY=

# Extension: elevenlabs_tts
# ElevenLabs TTS key
ELEVENLABS_TTS_KEY=

# Extension: fish_audio_tts
# Fish.audio TTS key
FISH_AUDIO_TTS_KEY=

# Extension: minimax_tts
# Minimax TTS key
MINIMAX_TTS_API_KEY=
MINIMAX_TTS_GROUP_ID=

# Extension: bytedance_tts
BYTEDANCE_TTS_APPID=
BYTEDANCE_TTS_TOKEN=

# Extension: dubverse_tts
# Dubverse TTS key
DUBVERSE_TTS_KEY=

# ------------------------------
# Tools
