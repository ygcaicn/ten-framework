{
  "type": "extension",
  "name": "oceanbase_powerrag_python",
  "version": "0.2.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.11"
    },
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "0.7"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "**.tent",
      "**.py",
      "README.md",
      "tests/**"
    ]
  },
  "tags": [
    "python",
    "template",
    "llm"
  ],
  "api": {
    "interface": [
      {
        "import_uri": "../../system/ten_ai_base/api/llm-interface.json"
      }
    ],
    "property": {
      "properties": {
        "user_id": {
          "type": "string"
        },
        "base_url": {
          "type": "string"
        },
        "api_key": {
          "type": "string"
        },
        "ai_database_name": {
          "type": "string"
        },
        "collection_id": {
          "type": "string"
        },
        "failure_info": {
          "type": "string"
        }
      }
    }
  }
}