{
  "type": "extension",
  "name": "minimax_tts_websocket_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.10"
    },
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "=0.6.25"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "**.tent",
      "**.py",
      "README.md",
      "tests/**"
    ]
  },
  "api": {
    "interface": [
      {
        "import_uri": "../../system/ten_ai_base/api/tts-interface.json"
      }
    ],
    "property": {
      "properties": {
        "params": {
          "type": "object",
          "properties": {
            "api_key": {
              "type": "string"
            },
            "group_id": {
              "type": "string"
            },
            "model": {
              "type": "string"
            },
            "voice_id": {
              "type": "string"
            },
            "sample_rate": {
              "type": "int64"
            },
            "url": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}